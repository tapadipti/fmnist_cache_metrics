{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
    "data": {
      "values": "<DVC_METRIC_DATA>"
    },
  
    "width": 400,
    "height": 100,
  
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "<DVC_METRIC_X>",
        "type": "nominal"
      },
      "y": {
        "field": "<DVC_METRIC_Y>",
        "type": "quantitative"
      },
      "row": {"field": "rev"},
      "color": {
        "field": "<DVC_METRIC_X>"
      }
    }
}
  